CREATE TABLE T1(key STRING, val STRING) STORED AS TEXTFILE

CREATE TABLE T2(key STRING, val STRING) STORED AS TEXTFILE

CREATE TABLE T3(key STRING, val STRING) STORED AS TEXTFILE


